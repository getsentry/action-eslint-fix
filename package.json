{
  "name": "@getsentry/action-eslint-fix",
  "version": "0.0.0",
  "private": true,
  "description": "Lint and fix javascript",
  "main": "lib/main.js",
  "scripts": {
    "build": "yarn tsc",
    "format": "yarn prettier --write **/*.ts",
    "lint": "yarn eslint src/**/*.ts",
    "dist": "yarn build && yarn ncc build -e eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getsentry/action-eslint-fix.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "getsentry",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@octokit/webhooks": "^7.12.0",
    "@types/node": "^12.7.12",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vercel/ncc": "^0.24.1",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "js-yaml": "^3.14.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
