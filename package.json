{
  "name": "@getsentry/action-eslint-fix",
  "version": "0.0.0",
  "private": true,
  "description": "Lint and fix javascript",
  "main": "lib/main.js",
  "scripts": {
    "build": "yarn tsc",
    "format": "yarn prettier --write **/*.ts",
    "format-check": "yarn prettier --check **/*.ts",
    "lint": "yarn eslint src/**/*.ts",
    "test": "yarn jest",
    "all": "yarn build && yarn ncc build && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getsentry/action-eslint-fix.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "getsentry",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.3",
    "@actions/github": "^2.1.1",
    "@actions/glob": "^0.1.0"
  },
  "devDependencies": {
    "@octokit/webhooks": "^7.1.0",
    "@types/eslint": "^6.1.8",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
